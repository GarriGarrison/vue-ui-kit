{
  "name": "@garrigarrison/vue-ui-kit",
  "private": false,
  "version": "0.4.0",
  "description": "UI kit for Vue 3",
  "engines": {
    "node": ">= 16.14.0",
    "npm": ">= 7.0.0",
    "yarn": "^1.22.19"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-ui-kit.umd.js",
  "module": "./dist/vue-ui-kit.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-ui-kit.es.js",
      "require": "./dist/vue-ui-kit.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GarriGarrison/vue-ui-kit.git"
  },
  "author": {
    "name": "Garrison",
    "email": "garrisonford@yandex.ru",
    "url": "https://github.com/GarriGarrison"
  },
  "homepage": "https://github.com/GarriGarrison/vue-ui-kit#readme",
  "bugs": {
    "url": "https://github.com/GarriGarrison/vue-ui-kite/issues"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build": "npm run clean && vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types && npm run dell",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint ./src/**/*.{js,ts,jsx,tsx,vue}",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:fix": "eslint ./src/** --fix",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "lint:markup": "vue-tsc --noEmit",
    "test": "vitest run --coverage.enabled --coverage.lines=50",
    "test-coverage": "vitest --coverage",
    "test-ui": "npx vitest --ui",
    "test:unit": "vitest --environment jsdom",
    "jest": "vitest",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "dell": "rm dist/vite.config.d.ts && rm dist/vite.svg && rm dist/types/App.vue.d.ts && rm dist/types/main.d.ts && rimraf dist/types/common",
    "storybook": "storybook dev",
    "build-storybook": "storybook build",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "vue": "^3.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@garrigarrison/vue-ui-kit": "^0.3.0",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
    "@storybook/addon-actions": "^7.6.2",
    "@storybook/addon-essentials": "^7.6.2",
    "@storybook/addon-links": "^7.6.2",
    "@storybook/addon-mdx-gfm": "^7.6.2",
    "@storybook/cli": "^7.6.2",
    "@storybook/vue3": "^7.6.2",
    "@storybook/vue3-vite": "^7.6.2",
    "@testing-library/vue": "^8.0.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.10.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/eslint-config-airbnb": "^7.0.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "storybook": "^7.6.2",
    "stylelint": "^15.11.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.1.0",
    "stylelint-scss": "^5.3.1",
    "types-spring": "^2.0.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vitest": "^0.34.6",
    "vue-eslint-parser": "^9.3.2",
    "vue-loader": "^17.3.1",
    "vue-tsc": "^1.8.24"
  },
  "keywords": [
    "vue",
    "ui-kit"
  ],
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions",
    "> 5%",
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "Chrome >= 115",
    "Firefox >= 115"
  ]
}
