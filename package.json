{
  "name": "@garrigarrison/vue-ui-kit",
  "private": false,
  "version": "0.0.1",
  "description": "UI kit for Vue 3",
  "type": "module",
  "engines": {
    "node": ">= 16.14.0",
    "npm": ">= 7.0.0",
    "yarn": "^1.22.19"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-ui-kit.umd.js",
  "module": "./dist/vue-ui-kit.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-ui-kit.es.js",
      "require": "./dist/vue-ui-kit.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@GarriGarrison"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GarriGarrison/vue-ui-kit.git"
  },
  "author": {
    "name": "Garrison",
    "email": "garrisonford@yandex.ru",
    "url": "https://github.com/GarriGarrison"
  },
  "homepage": "https://github.com/GarriGarrison/vue-ui-kit#readme",
  "bugs": {
    "url": "https://github.com/GarriGarrisonvue-ui-kite/issues"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build": "npm run clean && vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint ./src/**/*.{js,ts,jsx,tsx,vue}",
    "lint:fix": "eslint ./src/** --fix",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "test": "exit 0",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.3.1",
    "types-spring": "^2.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vue-tsc": "^1.8.22"
  },
  "keywords": [
    "vue",
    "ui-kit"
  ],
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions",
    "> 5%",
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "Chrome >= 100",
    "Firefox >= 100"
  ]
}
